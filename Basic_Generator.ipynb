        "def simple_generator():\n",
        "    for i in range(1, 6):\n",
        "        yield i\n",
        "\n",
        "#Using the generator\n",
        "gen = simple_generator()\n",
        "\n",
        "#printing values\n",
        "for value in gen:\n",
        "    print(value)"
